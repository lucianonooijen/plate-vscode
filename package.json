{
    "name": "language-plate",
    "displayName": "Plate Languages Support",
    "description": "Plate Languages Syntax Highlighter",
    "version": "0.0.1",
    "icon": "icon.png",
    "publisher": "lucianonooijen",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "plate",
            "aliases": ["HTML Plate", "plate"],
            "extensions": [".plate"],
            "configuration": "./plate.configuration.json"
        }],
        "grammars": [{
            "language": "plate",
            "scopeName": "text.html.plate",
            "path": "./syntaxes/plate.tmLanguage"
        }]
    }
}
